{
	"name": "lindemannrock/craft-formie-paragraph-field",
	"description": "Paragraph field for Formie - Provides a multi-line paragraph field type for Craft CMS Formie plugin",
	"type": "craft-plugin",
	"version": "3.1.1",
	"keywords": [
		"craft",
		"cms",
		"craftcms",
		"craft-plugin",
		"formie",
		"paragraph field",
		"textarea"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Bilal Hary Lindemann",
			"email": "bh@lindemannrock.com",
			"homepage": "https://lindemannrock.com",
			"role": "Developer"
		}
	],
	"support": {
		"email": "support@lindemannrock.com",
		"issues": "https://github.com/LindemannRock/craft-formie-paragraph-field/issues",
		"source": "https://github.com/LindemannRock/craft-formie-paragraph-field",
		"docs": "https://github.com/LindemannRock/craft-formie-paragraph-field/blob/main/README.md",
		"rss": "https://github.com/LindemannRock/craft-formie-paragraph-field/commits/main.atom"
	},
	"require": {
		"php": "^8.2",
		"craftcms/cms": "^5.0.0",
		"verbb/formie": "^3.0.0",
		"lindemannrock/craft-plugin-base": "^5.0"
	},
	"require-dev": {
		"craftcms/ecs": "dev-main",
		"craftcms/phpstan": "dev-main"
	},
	"autoload": {
		"psr-4": {
			"lindemannrock\\formieparagraphfield\\": "src/"
		}
	},
	"scripts": {
		"phpstan": "phpstan --ansi --memory-limit=1G",
		"check-cs": "ecs check --ansi",
		"fix-cs": "ecs check --fix --ansi"
	},
	"extra": {
		"name": "Formie Paragraph Field",
		"handle": "formie-paragraph-field",
		"developer": "LindemannRock",
		"developerUrl": "https://lindemannrock.com",
		"documentationUrl": "https://github.com/LindemannRock/craft-formie-paragraph-field/blob/main/README.md",
		"changelogUrl": "https://github.com/LindemannRock/craft-formie-paragraph-field/blob/main/CHANGELOG.md",
		"class": "lindemannrock\\formieparagraphfield\\FormieParagraphField"
	}
}
